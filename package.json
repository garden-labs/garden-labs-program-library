{
  "name": "@garden-labs/sdk",
  "version": "1.0.0",
  "description": "",
  "main": "build/sdk/index.js",
  "types": "build/sdk/index.d.ts",
  "files": [
    "build",
    "Anchor.toml"
  ],
  "scripts": {
    "build:sdk": "tsc --project sdk/tsconfig.json",
    "build:programs": "anchor build && util/bash/build/advanced-token-metadata.sh",
    "set-network:localnet": "solana config set --url http://localhost:8899",
    "set-network:devnet": "solana config set --url https://api.devnet.solana.com",
    "deploy:devnet": "npm run set-network:devnet && util/bash/deploy/devnet/all.sh",
    "write-buffer:prio-fees:staked-rpc": "solana program write-buffer --url https://deploy-qos.mainnet.rpcpool.com --with-compute-unit-price 200000 --max-sign-attempts 10000 --",
    "recover-buffer": "solana-keygen recover -o buffer.json",
    "recover-all-buffers": "solana program close --buffers",
    "verify:get-hash:executable": "solana-verify get-executable-hash  --",
    "verify:get-hash:buffer:mainnet": "solana-verify get-buffer-hash --url https://api.mainnet-beta.solana.com $(solana-keygen pubkey buffer.json)",
    "verify:get-hash:program:mainnet": "solana-verify get-program-hash --url https://api.mainnet-beta.solana.com --",
    "deploy:mainnet": "solana program deploy --with-compute-unit-price 200000 --max-sign-attempts 10000 --buffer $(solana-keygen pubkey buffer.json) --program-id --",
    "upgrade:mainnet": "solana program upgrade $(solana-keygen pubkey buffer.json) --",
    "get-size:executable": "stat -f %z --",
    "get-size:program": "solana program show --",
    "resize": "solana program extend --",
    "start:localnet": "solana-test-validator",
    "restart:localnet": "solana-test-validator -r",
    "test:localnet": "npm run set-network:localnet && util/bash/build/advanced-token-metadata.sh && TEST_ENV=localnet anchor test",
    "test:localnet:vending-machine": "TEST_INCLUDE_PATH=vending-machine/test/**/*.ts npm run test:localnet",
    "test:localnet:keep-open": "npm run test:localnet -- --skip-local-validator",
    "test:devnet": "npm run set-network:devnet && TEST_ENV=devnet npm run vitest run",
    "test:devnet:redeploy": "npm run build:programs && npm run deploy:devnet && npm run test:devnet",
    "lint": "eslint ."
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@coral-xyz/borsh": "^0.30.1",
    "@iarna/toml": "^2.2.5",
    "@solana/codecs": "2.0.0-preview.2",
    "@solana/spl-token": "0.4.3",
    "@solana/spl-token-metadata": "0.1.3",
    "@solana/spl-type-length-value": "0.1.0",
    "@solana/web3.js": "^1.87.6",
    "app-root-path": "^3.1.0",
    "dotenv": "^16.3.1",
    "expand-tilde": "^2.0.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/expand-tilde": "^2.0.2",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
